<asp:Content ID="Content3" ContentPlaceHolderID="ContentPlaceHolderScript" runat="server">
    <script src="http://echarts.baidu.com/build/dist/echarts-all.js"></script>
        <script type="text/javascript">


            ///////////////////////////////////////////////
            function reLocate(index) {               //用js算法来让左边导航栏在屏幕小于一定分辨率时隐藏
                window.location.hash = index;
                window.scrollBy(0, -70);              //相对向下位移70px
            }
            /////////////////控制旁边guider是否显示，用法暂时看不懂 ……mark……///////////////////
            $(document).ready(function () {                    //每次窗口宽度改变则调用
                window.onresize = guiderControl;
                guiderControl();
            });
            function guiderControl() {
                if ($(window).width() <= 750) {
                    $(".guider").hide();
                } else {
                    $(".guider").show();
                }
            }
            ////////
            //////////////////////////////////////
            $(document).scroll(function () {                           //这段代实现动态滚动监视功能。对应的<h2>与对应的.guider里的<butotn>相对应。
                var hArray = document.getElementsByTagName('h2');      //条件是不能再其他地方再使用<h2>标签，
                for (var i = 0; i < hArray.length; i++) {              //获取<h2>标签总数
                    var temp = 'head' + i;                             //用temp存储字符，然后getElementById找到对应的<h2>Y坐标；
                    var tid = document.getElementById(temp);
                    while ($(document).scrollTop() >= tid.offsetTop - 2) { //while循环添加active类。//offsetTop.是js的用法；offset().top:jquery
                        $(".guider button").removeClass('active');     //注意先删除掉所有active类，在给对应的<button>添加active类；
                        var t = '.guider button:eq(' + i + ')';
                        $(t).addClass('active');
                        break;
                    }
                }
            });
            $(document).ready(function () {
                $('#backTop').on('click', function () {
                    $(".guider button").removeClass('active');
                });
            });
            ///////////////////////////////////////////////

    </script>

